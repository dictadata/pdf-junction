{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "name": "pdf parser",
      "program": "${workspaceFolder}/test/pdf/parser.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "pdf codify",
      "program": "${workspaceFolder}/test/pdf/codify.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "pdf retrieve",
      "program": "${workspaceFolder}/test/pdf/retrieve.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "pdf transfer",
      "program": "${workspaceFolder}/test/pdf/transfers.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    }
  ]
}
